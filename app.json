{
  "expo": {
    "name": "Coffio",
    "slug": "coffio",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/coffio-logo-dark.png",
    "splash": {
      "image": "./assets/coffio-splash-transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#252a34"
    },
    "updates": {
      "fallbackToCacheTimeout": 2000
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.travissaylor.coffeeRatios",
      "buildNumber": "2.1.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.travissaylor.coffeeRatios",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/coffio-logo-dark-transparent.png",
        "backgroundColor": "#252a34"
      },
      "permissions": ["WRITE_EXTERNAL_STORAGE", "READ_EXTERNAL_STORAGE"]
    },
    "description": ""
  }
}
